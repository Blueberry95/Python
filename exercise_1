# Find x with arguments a and b, where b not equals 0

import sys
import math
a = float(sys.argv[1])
b = float(sys.argv[2])
e = 2.71
print("arg1 =", a, ", arg2 =", b)
if b == 0:
    print("value b is zero")
    exit(1)
else:
    print("value isn't zero")

x = (math.sqrt(a*b) / (e ** a * b)) + a * e ** ((2 * a) / b)
print(x)
